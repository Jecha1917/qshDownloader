{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "WorkerConfig": {
    "HistoryPath": "C:\\Trading\\History",
    "UrlQshServers": [
      "http://erinrv.qscalp.ru",
      "http://qsh.qscalp.ru/Techcap"
    ],
    "ParseQshUrls": "\"([\\w\\/]*[\\/\\-\\d]+([\\.\\-\\w]+\\.qsh))\"",
    "ParseDate": ">(\\d{4}-\\d{2}-\\d{2})<",
    "HoursInterval": 24
  }
}
