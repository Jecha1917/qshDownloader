{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "WorkerConfig": {
    "HistoryPath": "D:\\Trading\\History",
    "ServerUrls": [
      "http://erinrv.qscalp.ru",
      "http://qsh.qscalp.ru/Techcap"
    ],
    "HoursInterval": 24
  },
  "DownloaderConfig": {
    "RetryCount": 10,
    "RetryDelay": 3000,
    "MaxParallelThreads": 16,
    "ParseFileUrls": "\"([\\w\\/]*[\\/\\-\\d]+([\\.\\-\\w]+\\.qsh))\"",
    "ParseDate": ">(\\d{4}-\\d{2}-\\d{2})<"
  }
}
